#include <stdio.h>
#include <cs50.h>
#include <string.h>

//lets specify that our program can take in command line arguments 
int main(int argc, string argv[])
{
    for(int i = 0; i < argc; i++)
    {
        for(int j = 0, n = strlen(argv[i]); j < n; j++)
        {
            printf("%c \n", argv[i][j]);
        }
    }
}